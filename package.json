{
  "name": "auth-tg-mini-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3069",
    "dev:https": "next dev --experimental-https",
    "build": "env-cmd -f .env.production next build",
    "build:dev": "env-cmd -f .env next build",
    "start": "next start",
    "deploy:dev": "./scripts/deployDevelopment.sh",
    "deploy:prod": "./scripts/deployProduction.sh",
    "prepare": "husky install",
    "lint": "eslint ./src --no-error-on-unmatched-pattern --ext .ts,.tsx,.less,.scss -c .eslintrc.json --fix",
    "prettier": "prettier --write '**/*.{js,cjs,jsx,ts,tsx,css,less,sass,html,json}'",
    "stylelint": "stylelint 'packages/*/src/**/*.{css,less,scss,sass}' --config .stylelintrc.cjs --fix",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@nextui-org/react": "^2.4.6",
    "@telegram-apps/sdk-react": "^1.1.3",
    "@web3auth/base": "^9.2.3",
    "@web3auth/base-provider": "^9.2.3",
    "@web3auth/single-factor-auth": "^9.0.0",
    "add": "^2.0.6",
    "blo": "^1.2.0",
    "eruda": "^3.0.1",
    "framer-motion": "^11.3.8",
    "next": "14.2.4",
    "normalize.css": "^8.0.1",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "^1.5.0",
    "tonweb": "^0.0.66",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/bs58": "^4.0.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "stylelint": "^15.10.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-formatters-html": "^1.1.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^33.0.0"
  },
  "lint-staged": {
    "*.{css,less,scss,sass}": [
      "stylelint --fix"
    ],
    "*.{ts,tsx,js,jsx,vue}": [
      "npx eslint -f table"
    ]
  }
}
